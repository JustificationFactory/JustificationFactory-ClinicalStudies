<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml"
         prefHeight="1e8" prefWidth="200" maxWidth="300"
         BorderPane.alignment="CENTER" type="javafx.scene.layout.Pane">
    <BorderPane>
    <top>
        <Label fx:id="title" text="Subject" BorderPane.alignment="CENTER" style="-fx-font-size: 30"/>
    </top>

    <center>
        <Accordion fx:id="acrdnExpSubject" BorderPane.alignment="CENTER"/>
    </center>

    <bottom>
        <Button fx:id="btnHistory" styleClass="bHistory" mnemonicParsing="false" onAction="#onClicShowHistory"
                text="History" BorderPane.alignment="CENTER"/>
    </bottom>
    </BorderPane>
</fx:root>